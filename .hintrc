{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "disown-opener": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scrollbar-width",
          "text-wrap",
          "text-wrap: balance"
        ]
      }
    ],
    "button-type": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not chrome <= 109",
    "not ios_saf <= 18",
    "not safari <= 18",
    "not samsung <= 26"
  ]
}